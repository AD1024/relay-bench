{
  "data": {
    "url": "tvm_benchmark/vta-resultsv2.csv",
    "format": { "type": "csv" }
  },
  "facet": {
    "column": { "field": "Model", "type": "nominal" }
  },
  "transform": [
    {
      "filter": { "field": "Model", "oneOf": ["ResNet-18", "ResNet-34", "ResNet-50"] }
    }
  ],
  "spec": {
    "layer": [
      {
        "mark": "bar",
        "encoding": {
        "x":      { "field": "Target × IR", "type": "nominal" },
        "y":      { "field": "ms_per_image", "type": "quantitative",
                    "aggregate": "mean",
                    "axis": {
                      "title": "Mean Inference Time (ms)"
                    } },
        "color": { "field": "Target × IR", "type": "nominal" }
        }
      }
    ]
  }
}
